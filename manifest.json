{
	"author": "dust1777",
	"name": "Logar pela token",
	"version": "1.0",
	"description": "Logue no discord apenas inserindo a token",
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"run_at": "document_idle",
			"js": ["contentScript.js"]
		}
	],
	"browser_action": {
		"default_popup": "index.html",
		
		"default_icon": {
			"16": "img/16.png",
			"32": "img/32.png",
			"48": "img/48.png",
			"128": "img/128.png"
		}
	},
	"icons": {
		"16": "img/16.png",
		"32": "img/32.png",
		"48": "img/48.png",
		"128": "img/128.png"
	},
	"manifest_version": 2
}
